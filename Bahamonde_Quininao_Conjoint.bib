	Journal = {Political Analysis},
	Keywords = {conjoint analysis,factorial experiments,survey design,survey experiments},
	Month = {apr},
	Number = {2},
	Pages = {207--221},
	Title = {{Measuring Subgroup Preferences in Conjoint Experiments}},
